XTerm.termName: xterm-256color

! force utf8
! XTerm.vt100.locale: true

! behaviour similar to other terminal emulators
!Xterm.vt100.metaSendsEscape: true
!

XTerm.vt100.backarrowKey: false
XTerm.ttymodes: erase ^?

XTerm.vt100.saveLines: 9999

!XTerm.vt100.scrollBar: true
!Xterm.vt100.scrollBar.width: 1
!xterm*rightScrollBar: true

XTerm.vt100.selectToClipboard:	true 

xterm*background:	Grey15
xterm*foreground:	white
XTerm*faceName:		UbuntuMono:style=regular:size=13:antialias=true
xterm*faceNameDoublesize:WenQuanYi Micro Hei:antialias=true:pixelsize=20
!XTerm*faceName:	IBMPlexMono:style=Regular:size=11:antialias=true
!XTerm*faceName:	Droid Sans Mono:size=11:antialias=true
!XTerm*faceName: 	DejaVu Sans Mono Book:size=11
!XTerm*faceName:	Incosolata:size=10.5:antialias=true
!XTerm*faceName:	Envy Code R:size=12:antialias=false
!XTerm*faceName:	xft:DejaVu Sans ExtraLight:pixelsize=14:antialias=false
!XTerm*faceName:	Monospace:size=11.5:antialias=false
!XTerm*faceName:	Bitstream Vera Sans Mono:size=9
!XTerm*faceName:	Terminus:pixelsize=14:antialias=true
!XTerm*font:		-xos4-terminus-medium-r-normal--18-180-72-72-c-100-iso10646-1
!Xterm*font: 		-artwiz-lime-medium-*-normal-*-17-*-100-100-m-0-iso8859-*
!Xterm*font:		-*-fixed-medium-r-*-*-18-*-*-*-*-*-iso8859-*
!Xterm*font:		-*-gohufont-medium-r-*-*-14-*-*-*-*-*-*-iso8859-*
!Xterm*font:		-*-tamsyn-medium-r-*-*-17-*-*-*-*-*-*-*
!Xterm*font:		-*-terminus-medium-r-*-*-18-*-*-*-*-*-*-*
!Xterm*renderFont: 	false

!Xft*hinting: true
!Xft*hintstyle: hintsfull
Xft*dpi: 96
Xfr*rgba: vrgb

XTerm.vt100.translations: #override \n\
Ctrl Shift <Key>C: copy-selection(CLIPBOARD)	\n\
Ctrl Shift <Key>V: insert-selection(CLIPBOARD)
